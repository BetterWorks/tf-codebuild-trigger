{
  "name": "tf-codebuild-trigger",
  "version": "1.0.1",
  "description": "a node.js lambda function that triggers AWS Codebuild project builds using github event payloads delivered via SNS",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist/* && webpack",
    "coverage": "nyc --reporter=lcov npm test",
    "lint": "eslint ./lib",
    "release": "standard-version",
    "start": "node .",
    "test": "mocha --recursive test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cludden/tf-codebuild-trigger.git"
  },
  "keywords": [
    "node",
    "starter",
    "kit"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cludden/tf-codebuild-trigger/issues"
  },
  "homepage": "https://github.com/cludden/tf-codebuild-trigger#readme",
  "dependencies": {
    "ajv": "^6.10.2",
    "app-container": "^1.1.2",
    "aws-sdk": "^2.599.0",
    "babel-loader": "^8.0.6",
    "bunyan": "^1.8.12",
    "lodash.attempt": "^4.2.0",
    "lodash.compact": "^3.0.1",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "source-map-support": "^0.5.16"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "faker": "^4.1.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "sinon": "^8.0.4",
    "standard-version": "^7.0.1",
    "webpack": "^4.41.5",
    "yazl": "^2.5.1"
  }
}
